#define CATCH_CONFIG_MAIN
#include "catch.hpp"
#include "RBTNode.hpp"
#include <iostream>

using namespace std;

TEST_CASE("Testing BSTNode"){
    SECTION("Constructor"){
        BSTNode<char, int> a = BSTNode<char, int>( 'a', 1);
        BSTNode<double, double> b = BSTNode<double, double>(63, 2);
        BSTNode<int, char> c = BSTNode<int, char> (2, 'c');
    }
}
